{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "no-unused-vars": "off",
        "no-underscore-dangle":"off",
        "class-methods-use-this":"off",
        "no-undef":"off",
        "camelcase":"off",
        "import/no-extraneous-dependencies": "off"
    }
}
